<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
	
			<bean class="converters.WarehouseToStringConverter"/>
			<bean class="converters.StringToWarehouseConverter"/>
			<bean class="converters.TaxToStringConverter"/>
			<bean class="converters.StringToTaxConverter"/>
			<bean class="converters.StringToShoppingCartConverter"/>
			<bean class="converters.ShoppingCartToStringConverter"/>
			<bean class="converters.StringToDistributorConverter"/>
			<bean class="converters.DistributorToStringConverter"/>
			<bean class="converters.StringToCustomerConverter"/>
			<bean class="converters.CustomerToStringConverter"/>
			<bean class="converters.StringToStockConverter"/>
			<bean class="converters.StockToStringConverter"/>
			<bean class="converters.StringToItemConverter"/>
			<bean class="converters.ItemToStringConverter"/>
			<bean class="converters.StringToCommentConverter"/>
			<bean class="converters.CommentToStringConverter"/>
			<bean class="converters.StringToMessageConverter"/>
			<bean class="converters.MessageToStringConverter"/>
			<bean class="converters.StringToFolderConverter"/>
			<bean class="converters.FolderToStringConverter"/>
			<bean class="converters.StringToActorConverter"/>
			<bean class="converters.ActorToStringConverter"/>
			<bean class="converters.StringToProducerConverter"/>
			<bean class="converters.ProducerToStringConverter"/>
			<bean class="converters.StringToShoppingLineConverter"/>
			<bean class="converters.ShoppingLineToStringConverter"/>
			<!-- <bean class="converters.StringToShippingCompanyConverter"/>
			<bean class="converters.ShippingCompanyToStringConverter"/> -->
	</util:list>
	
</beans>